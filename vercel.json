{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next",
        "config": { "distDir": ".next" }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/.next/server/app/$1.html" },
      { "src": "/", "dest": "/.next/server/app/index.html" }
    ]
  }